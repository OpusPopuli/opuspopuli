{
  "name": "qckstrt",
  "version": "1.0.0",
  "description": "Quick Start NextJs / NestJs Full-Stack running on AWS",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:ci": "pnpm -r test -- --coverage",
    "test:integration": "pnpm --filter backend test:integration",
    "test:integration:docker": "bash scripts/test-integration-docker.sh",
    "integration:up": "docker compose -f docker-compose-integration.yml up -d --build",
    "integration:down": "docker compose -f docker-compose-integration.yml down -v",
    "integration:logs": "docker compose -f docker-compose-integration.yml logs -f",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "pnpm -r lint",
      "pnpm -r format"
    ],
    "**/*.+(json|css)": [
      "pnpm -r format"
    ]
  },
  "dependencies": {
    "cockatiel": "^3.2.1"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1",
      "diff": ">=5.2.2",
      "@langchain/core": ">=1.1.8",
      "@modelcontextprotocol/sdk": ">=1.25.2",
      "tar": ">=7.5.4",
      "fast-xml-parser": ">=5.3.4",
      "lodash": ">=4.17.23",
      "undici": ">=7.18.2"
    }
  }
}
