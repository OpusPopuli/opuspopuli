{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev --port 3200",
    "prebuild": "pnpm lint",
    "format": "prettier --write \"app/**/*.tsx\"",
    "build": "next build --webpack",
    "start": "next start",
    "cf:build": "opennextjs-cloudflare build",
    "cf:preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf:deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:a11y": "jest --testPathPatterns=accessibility --no-coverage",
    "preinstall": "npx only-allow pnpm",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:a11y": "playwright test e2e/accessibility.spec.ts"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@apollo/client": "^4.0.10",
    "@deck.gl/core": "^9.2.9",
    "@deck.gl/layers": "^9.2.9",
    "@deck.gl/mapbox": "^9.2.9",
    "@serwist/next": "^9.5.3",
    "@simplewebauthn/browser": "^13.2.2",
    "apollo3-cache-persist": "^0.15.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^6.0.4",
    "i18next": "^25.7.3",
    "idb": "^8.0.3",
    "jwt-decode": "^4.0.0",
    "maplibre-gl": "^5.18.0",
    "next": "^16.1.5",
    "pmtiles": "^4.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.5.0",
    "react-map-gl": "^8.1.0",
    "serwist": "^9.5.3",
    "supercluster": "^8.0.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.17.0",
    "@opennextjs/cloudflare": "^1.16.5",
    "@playwright/test": "^1.57.0",
    "@serwist/build": "^9.5.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.0.1",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/supercluster": "^7.1.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.5",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "wrangler": "^4.59.2"
  },
  "overrides": {
    "eslint": "$eslint"
  }
}
