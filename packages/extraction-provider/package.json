  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/testing": "^11.1.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.5",
    "domhandler": "^5.0.3",
    "jest": "^30.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
