    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "qckstrt",
    "extraction",
    "text",
    "url",
    "provider"
  ],
  "license": "MIT",
  "dependencies": {
    "@qckstrt/common": "workspace:*",
