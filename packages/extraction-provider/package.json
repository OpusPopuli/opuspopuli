{
  "name": "@qckstrt/extraction-provider",
  "version": "0.1.0",
  "description": "Text extraction provider implementations for QCKSTRT platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
